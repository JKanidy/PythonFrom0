   "outputs": [],
   "execution_count": null,
   "source": [
    "a = int(input())\n",
    "b = int(input())\n",
    "i = 1\n",
    "\n",
    "while i <= (a * b) and i != 0:\n",
    "    if i % a == 0 and i % b == 0:\n",
    "        print(i)\n",
    "        i = 0\n",
    "    else:\n",
    "        i += 1"
